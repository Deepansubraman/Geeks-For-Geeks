class Solution {
  public:
    string isPrime(int n) {
        // code here
        //int count=0;
        if(n==1)
{
        return "No";
}
        for(int i=2;i*i<=n;i++)
        {
            if(n%i==0)
            {
                return "No";
            }
        }
        return "Yes";
    }
};
